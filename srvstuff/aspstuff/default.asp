var gwRankings = angular.module('gwRankings', ['angularUtils.directives.dirPagination']);


function RankingController($scope, $http) {
	$scope.rankList = "json/pro.json";
	$scope.tournamentList = "";
	$scope.currentPage = 1;
	$scope.pageSize = 50;	

	$scope.pageChangeHandler = function(num) {
		//console.log('rankings page changed to ' + num);
	};

	$scope.$watch('rankList', function() {
		
		if($scope.rankList.indexOf("http") > -1 || $scope.rankList.indexOf(".asp") > -1){
			window.location = $scope.rankList;
		} else {
			if($scope.rankList == 'json/boys.json' || $scope.rankList == 'json/girls.json'){
				$scope.colspan = 7
			} else if ($scope.rankList == 'json/men.json' || $scope.rankList == 'json/women.json') {
				$scope.colspan = 6
			} else {
				$scope.colspan = 5
			}
			
			$scope.q = ""
			$scope.r = ""
			$scope.s = ""
			$scope.t = ""
			
			if($scope.rankList == ''){
				$http.get($scope.tournamentList)
				.success(function(response) {$scope.tournaments = response;});
			} else {
				$http.get($scope.rankList)
				.success(function(response) {$scope.rankings = response;});
			}
		
			if($scope.rankList == 'json/boys.json'){
				$scope.title = "Boy's Junior Rankings"
				$scope.urlbar = "?t=boys"	
				$scope.tournurl = "?t=boyst"	
			}
			
			if($scope.rankList == 'json/girls.json'){
				$scope.title = "Girl's Junior Rankings"
				$scope.urlbar = "?t=girls"	
				$scope.tournurl = "?t=girlst"
			}
			
			if($scope.rankList == 'json/men.json'){
				$scope.title = "Men's Collegiate Individual Rankings"
				$scope.urlbar = "?t=mi"
				$scope.tournurl = "?t=mit"
			}
			
			if($scope.rankList == 'json/women.json'){
				$scope.title = "Women's Collegiate Individual Rankings"
				$scope.urlbar = "?t=wi"	
				$scope.tournurl = "?t=wit"
			}
			
			if($scope.rankList == 'json/mensteams.json'){
				$scope.title = "Men's Team Collegiate Rankings"
				$scope.urlbar = "?t=mt"
				$scope.tournurl = "?t=mtt"				
			}
			
			if($scope.rankList == 'json/womensteams.json'){
				$scope.title = "Women's Team Collegiate Rankings"
				$scope.urlbar = "?t=wt"
				$scope.tournurl = "?t=wtt"
			}
			
			if($scope.rankList == 'json/pro.json'){
				$scope.title = "Men's Professional Rankings"
				$scope.urlbar = "?t=world"	
				$scope.tournurl = "?t=worldt"
			}
			
			if($scope.rankList == 'json/lpga.json'){
				$scope.title = "Women's Professional Rankings"
				$scope.urlbar = "?t=lpga"	
				$scope.tournurl = "?t=lpgat"
			}

			
			if($scope.tournamentList == 'json/boyst.json'){
				$scope.title = "Boy's Junior Tournaments"
				$scope.urlbar = "?t=boyst"	
			}
			
			if($scope.tournamentList == 'json/girlst.json'){
				$scope.title = "Girl's Junior Tournaments"
				$scope.urlbar = "?t=girlst"	
			}
			
			if($scope.tournamentList == 'json/ment.json'){
				$scope.title = "Men's Collegiate Individual Tournaments"
				$scope.urlbar = "?t=mit"	
			}
			
			if($scope.tournamentList == 'json/woment.json'){
				$scope.title = "Women's Collegiate Individual Tournaments"
				$scope.urlbar = "?t=wit"	
			}
			
			if($scope.tournamentList == 'json/mensteamst.json'){
				$scope.title = "Men's Team Collegiate Tournaments"
				$scope.urlbar = "?t=mtt"	
			}
			
			if($scope.tournamentList == 'json/womensteamst.json'){
				$scope.title = "Women's Team Collegiate Tournaments"
				$scope.urlbar = "?t=wtt"	
			}
			
			if($scope.tournamentList == 'json/prot.json'){
				$scope.title = "Men's Professional Tournaments"
				$scope.urlbar = "?t=worldt"	
			}
			
			if($scope.tournamentList == 'json/lpgat.json'){
				$scope.title = "Women's Professional Tournaments"
				$scope.urlbar = "?t=lpgat"	
			}

			window.history.pushState("", $scope.title, $scope.urlbar);

		}
	});
}

function OtherController($scope) {
	$scope.pageChangeHandler = function(num) {
		console.log('going to page ' + num);
	};
}

</script>
	
